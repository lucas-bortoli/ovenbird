{
    "name": "ovenbird",
    "author": "isbadatenglish",
    "description": "Electron + TypeScript file manager",
    "license": "MIT",
    "scripts": {
        "compile-ts": "tsc",
        "compile-scss": "sass src/scss/:dist/css/",
        "app": "cp -rf src/*.html dist/ && npm run compile-scss && npm run compile-ts && electron dist/main.js"
    },
    "dependencies": {
        "@types/mz": "^2.7.0",
        "electron": "^11.5.0",
        "moment": "^2.24.0",
        "mz": "^2.7.0",
        "node-disk-info": "^1.0.2",
        "sass": "^1.23.7",
        "typescript": "^3.7.3"
    }
}
